<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.magicbeans.collaboration.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.magicbeans.collaboration.entity.User">
    <result column="id" property="id" />
    <result column="create_time" property="createTime" />
    <result column="update_time" property="updateTime" />
        <result column="nickName" property="nickName" />
        <result column="avatar" property="avatar" />
        <result column="phone" property="phone" />
        <result column="pwd" property="pwd" />
        <result column="sex" property="sex" />
        <result column="cityId" property="cityId" />
        <result column="address" property="address" />
        <result column="parentId" property="parentId" />
        <result column="agencyUserId" property="agencyUserId" />
        <result column="agencyIsEdit" property="agencyIsEdit" />
        <result column="agencyIsDel" property="agencyIsDel" />
        <result column="agencyIsJurisdiction" property="agencyIsJurisdiction" />
        <result column="remark" property="remark" />
        <result column="token" property="token" />
        <result column="deviceToken" property="deviceToken" />
        <result column="deviceType" property="deviceType" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
    id,
    create_time AS createTime,
    update_time AS updateTime,
        nickName, avatar, phone, pwd, sex, cityId, address, parentId,
	agencyUserId, agencyIsEdit, agencyIsDel, agencyIsJurisdiction, remark,token,deviceToken,deviceType
    </sql>

</mapper>
